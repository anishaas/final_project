                    </li><!--end /menu-li -->
                    <li>
                        <a href="#">
                            <div class="gui-icon"><i class="fa fa-music"></i></div>
                            <span class="title">Manage Songs</span>
                        </a>
                    </li><!--end /menu-li -->
                    <li>
                        <a href="#">
                            <div class="gui-icon"><i class="fa fa-navicon"></i></div>
                            <span class="title">Manage Albums</span>
                        </a>
                    </li><!--end /menu-li -->
                    <li>
                        <a href="#">
                            <div class="gui-icon"><i class="fa fa-paint-brush"></i></div>
                            <span class="title">Manage Artists</span>
                        </a>
                    </li><!--end /menu-li -->
                    <li>
                        <a href="#">
                            <div class="gui-icon"><i class="fa fa-group"></i></div>
